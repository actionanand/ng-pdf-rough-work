{
  "/api/v1/static/pdf/local": {
    "target": "https://www.africau.edu/images/default/sample.pdf",
    "secure": false,
    "logLevel": "debug",
    "changeOrigin": true,
    "pathRewrite": {
      "^/api/v1/static/pdf/local": ""
    }
  },
  "/api/v1/static/pdf/remote": {
    "target": "https://www.africau.edu/images/default/sample.pdf",
    "secure": false,
    "logLevel": "debug",
    "changeOrigin": true,
    "pathRewrite": {
      "^/api/v1/static/pdf/remote": ""
    }
  },
  "/somepath/api/*": {
    "target": "http://localhost:5201",
    "secure": false,
    "logLevel": "debug",
    "changeOrigin": true,
    "pathRewrite": {
      "^/somepath/api": "/api"
    }
  }
}
